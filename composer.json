{
    "name": "mkhvsl/mv-users-api-test",
    "description": "WordPress Users API Test Plugin",
    "require": {
        "inpsyde/modularity": "^1.7"
    },
    "require-dev": {
        "inpsyde/php-coding-standards": "^1.0",
        "phpunit/phpunit": "^9.6",
        "brain/monkey": "2.*",
        "johnpbloch/wordpress-core": "^6.4"
    },
    "license": "GPL-2.0+",
    "autoload": {
        "psr-4": {
            "Mkhvsl\\MvUsersApiTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mkhvsl\\MvUsersApiTest\\Tests\\": "tests/src/",
            "Mkhvsl\\MvUsersApiTest\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "authors": [
        {
            "name": "Mykhailo Vasylenko",
            "email": "mkhvsl@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
